#include <kipr/wombat.h>

int main()
{
    enable_servos();
    set_servo_position(0,1900);
    wait(10500);
    close2();
    wait(3500);
    open2();
    wait(6000);
    lift();
    wait(5500);
    open2();
    wait(100);
    close2();
    wait(6500);
    open2();
    wait(10000);
    close2();
    wait(5000);
    open2();
    
    return 1;
}
void wait(int mill){
    wait_for_milliseconds(mill);
}
void close2(){
    set_servo_position(0,200);
}
void open2(){
    set_servo_position(0,900);
}

void lift(){
    motor(0,-65);
    wait(1250);
    motor(0,0);
}

void lower(){
    motor(0,65);
    wait(1250);
    motor(0,0);
}
    



